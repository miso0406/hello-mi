        "import random\n",
        "\n",
        "answer = random.randint(1, 100)\n",
        "\n",
        "while True:\n",
        "    guess = int(input(\"숫자를 맞춰보세요(1~100): \"))\n",
        "\n",
        "    if guess > answer:\n",
        "        print(\"너무 큽니다.\")\n",
        "    elif guess < answer:\n",
        "        print(\"너무 작습니다.\")\n",
        "    else:\n",
        "        print(\"정답입니다!\")\n",
        "        break"
